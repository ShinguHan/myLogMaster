{
  "name": "Dynamic Alarm with Variables",
  "steps": [
    {
      "action": "send",
      "message": "S1F13"
    },
    {
      "action": "expect",
      "message": "S1F14",
      "save_to_context": {
        "key": "comm_ack_code",
        "path": "body[0].value"
      }
    },
    {
      "action": "log",
      "message": "Saved COMMACK to variable 'comm_ack_code'."
    },
    {
      "action": "send",
      "message": "S5F1",
      "params": {
        "ALID": "context['comm_ack_code']"
      }
    },
    {
      "action": "expect",
      "message": "S5F2"
    }
  ]
}